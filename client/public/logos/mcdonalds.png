<amp-ad type="doubleclick" data-slot="/18190176,22755035103/AdThrive_Content/63ceb7f86d396318634ce956" json='{"targeting":{"siteId":"63ceb7f86d396318634ce956","siteName":"1000 Logos","amp_type":"rtc", "decref":"240", "location":"Content","sequence":10,"refresh":"false","amp":"true"}}'
